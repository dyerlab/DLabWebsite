@article{ DeSaixEtAl2019,
Author = {DeSaix, Matthew G. and Bulluck, Lesley P. and Eckert, Andrew J. and
   Viverette, Catherine B. and Boves, Than J. and Reese, Jessica A. and
   Tonra, Christopher M. and Dyer, Rodney J.},
Title = {{Population assignment reveals low migratory connectivity in a weakly
   structured songbird}},
Journal = {{MOLECULAR ECOLOGY}},
