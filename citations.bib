@book{ Dyer2015,
   abstract = {© 2016 by John Wiley  &  Sons Ltd. This chapter examines landscape genetic studies in plant population genetics. The main focus here is on the discussion of approaches and examples showing how population genetic structure can be influenced by spatial and ecologically heterogeneity. The chapter presents case studies directed towards the biological inferences regarding population genetic structure that are gained from the inclusion of spatial and ecological contexts. It takes a broader, and perhaps more comprehensive, view of what would be considered landscape genetics than is commonly used. An arbitrary partition is imposed in the chapter based on the spatial and temporal extent of the processes being examined, either contemporary or historical, straddling both population genetic and phylogeographic domains. The chapter closes with a short discussion on future research and potentially understudied aspects of plant landscape genetic analyses.},
   author = {R.J. Dyer},
   doi = {10.1002/9781118525258.ch11},
   isbn = {9781118525258},
   journal = {Landscape Genetics: Concepts, Methods, Applications},
   keywords = {Contemporary population genetic processes,Historical population genetic processes,Landscape genetics,Plant population genetics},
   title = {Landscapes and Plant Population Genetics},
   year = {2015},
}
@book{ Murphy2015,
   abstract = {© 2016 by John Wiley  &  Sons Ltd. Landscape genetic data are by nature graph-like in structure; graph approaches are extremely powerful for a wide range of landscape genetic applications. Graph applications have great potential to address landscape genetics questions in evolution, ecology, and conservation. This chapter discusses three applications of graph approaches: using graph methods to describe population genetic structure, testing hypotheses of gene flow among nodes, and assessing functional connectivity using a graph structure. Graph applications in landscape genetics have vast potential. The chapter analyzes several important considerations for researchers and managers wishing to apply graphs in a landscape genetics context. In graphs based upon statistical models, like population graphs, the statistical power of the analysis is based upon the sampling. In a graph analysis, it is possible to identify which nodes are most important to graph structure, what nodes are needed to maintain connectivity, and how the landscape impedes or promotes connectivity through the network.},
   author = {M. Murphy and R. Dyer and S.A. Cushman},
   doi = {10.1002/9781118525258.ch10},
   isbn = {9781118525258},
   journal = {Landscape Genetics: Concepts, Methods, Applications},
   keywords = {Graph theory,Landscape genetics,Network connectivity,Network models,Population graphs},
   title = {Graph Theory and Network Models in Landscape Genetics},
   year = {2015},
}
